{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["node", "jest", "jest-test-each"],
    "baseUrl": "./",
    "paths": {
      "cy-local": ["../src"],
      "cy-local/*": ["../src/*"]
    }
  },

  "include": ["./", "../node_modules/jest", "node_modules/jest-test-each/dist/index.d.ts"]
}
